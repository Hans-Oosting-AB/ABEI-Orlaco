, a.Description
, a.Referencenumber
, a.FreeIntField_01 AS BkstkNr
, a.Amount
, a.FreeBoolField_01 as Waiting
, a.FreeTextField_01 as WaitingReason
, a_first.first_created
, a.syscreated
, h.Fullname
, ISNULL(h.mail, 'd.foster@orlaco.com') as Mail
FROM dbo._AB_sy_Absences a WITH (NOLOCK)
CROSS APPLY (
    -- FreeGuidField_02 in Absences, Synergy (IFR request)
    -- Sysguid in Amutak, Globe (Financial Entry)
    SELECT MIN(a2.syscreated) as first_created
    FROM dbo._AB_sy_Absences a2 WITH (NOLOCK)
    WHERE a2.Type = a.Type
        AND a2.FreeGuidField_02 = a.FreeGuidField_02
    GROUP BY a2.FreeGuidField_02
) a_first
LEFT JOIN [orlaco].[dbo].[humres] h with (nolock) on a.empid = h.res_id
WHERE a.Type = 260  AND a.Status = 0 -- Open (No need to change when approved or finished)
    AND ((ISNULL(FreeBoolField_01, 0) = 0 AND DATEADD(DAY, 7, a_first.first_created) &lt; GETDATE()) -- 7 days when not waiting
        OR (ISNULL(FreeBoolField_01, 0) = 1 AND DATEADD(DAY, 30, a_first.first_created) &lt; GETDATE())) -- 30 days when waiting
order by Mail, a.HID
