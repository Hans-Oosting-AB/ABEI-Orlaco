FROM (
    SELECT T.AssignedTo,
    CASE WHEN ISNULL(U.U_ULOGIN,'') = '' or U.U_ULOGIN = '-' THEN 'j.vaarkamp@orlaco.com' ELSE U.U_ULOGIN END as Mail 
    from _ORL_vw_TaskPerProject T
    inner join USTAMM U ON U_NAME = AssignedTo
    where PlannedFinishDate &lt; GETDATE()+7 
    UNION
    Select T.AssignedTo,
    CASE WHEN ISNULL(U.U_ULOGIN,'') = '' or U.U_ULOGIN = '-' THEN 'j.vaarkamp@orlaco.com' ELSE U.U_ULOGIN END as Mail 
    from _ORL_vw_TaskPerProcess T
    inner join USTAMM U ON U_NAME = AssignedTo
    where PlannedFinishDate &lt; GETDATE()+7 
) A
