o.debnr, 
ord_debtor_name, 
r.artcode,
r.oms45,
r.esr_aantal as Quantity,
r.afldat,
h.fullname as AccMan, 
Case when o.ord_soort = 'V' then 'Order' when o.ord_soort = 'Q' then 'Quotation' end as OrdSoort,
ISNULL(h.mail ,'iss@orlaco.com')as AccManMail, 
hh.fullname as Creator, 
ISNULL(hh.mail,'iss@orlaco.com') as CreatorMail 
from       [400]..orkrg o with (NOLOCK)
inner join [400]..orsrg r with (NOLOCK) on r.ordernr = o.ordernr
inner join [400]..cicmpy c with (NOLOCK) on c.debnr = o.debnr
left join  [orlaco]..humres h with (NOLOCK) on h.res_id = c.cmp_acc_man
inner join [orlaco]..humres hh with (NOLOCK) on hh.res_id = o.syscreator
where -- o.debnr NOT IN ('997354', '999832', '969242')
 o.afgehandld = 0  and r.syscreated &gt;= '2022-07-01' 
 and o.ordernr not in (select  ordernr from [400]..orsrg rr with (NOLOCK) where  rr.syscreated &gt;= '2022-07-01' and rr.artcode like 'TXT-SPOT%' and (rr.prijs83 = 26.07 or rr.prijs83 = 26.68)) 
 and ((o.ord_soort = 'V' and r.pakbon_afg = 0  and r.afldat &lt; '2023-03-01') OR  (o.ord_soort = 'Q' ) )
 and r.artcode IN ('0224000')
 
UNION

