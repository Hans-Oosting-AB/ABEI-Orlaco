<?xml version="1.0" encoding="utf-8"?><jobs><instanceguid>d9025c07-3898-433f-88ee-c5899cf947e7</instanceguid><job><id>492</id><jobname>Orlaco Data Quality</jobname><disable>False</disable><emailbody /><emailmode>1</emailmode><emailsubject /><emailto>lennart.meeuse@stoneridge.com</emailto><remarks></remarks><schedulename /><schedulesettings>freq=1;timescale=once;startat=08:00:00;beginat=00:00:00;endat=23:59:59;day1=-1;day2=-1;day3=-1;day4=-1;day5=-1;day6=0;day7=0;enabled=-1</schedulesettings><settings></settings><settingsxml><settings><schedule><allowmultipleprocessorinstances>false</allowmultipleprocessorinstances></schedule><general><maxjobstepactions>200</maxjobstepactions></general><jobparams /></settings></settingsxml><steps><step><id>2982</id><entity>-none-</entity><action>create</action><source>1</source><target>6</target><datasource>SELECT '400' AS DB, cp.textfield5, cp.cnt_f_name, cp.cnt_l_name, cp.FullName,
c.cmp_code, c.cmp_name, c.cmp_status, c.cmp_type
FROM [400]..cicntp cp with (NOLOCK) 
INNER JOIN [400]..cicmpy c with (NOLOCK) on c.cmp_wwn = cp.cmp_wwn
WHERE cmp_status = 'A' AND cmp_type = 'C' AND cp.textfield5 IN(
    select a.textfield5 from [400]..cicntp a  with (NOLOCK) group by a.textfield5 having count(*) &gt; 1)

UNION
SELECT '483' AS DB, cp.textfield5, cp.cnt_f_name, cp.cnt_l_name, cp.FullName,
c.cmp_code, c.cmp_name, c.cmp_status, c.cmp_type
FROM [483]..cicntp cp with (NOLOCK) 
INNER JOIN [483]..cicmpy c with (NOLOCK) on c.cmp_wwn = cp.cmp_wwn
WHERE cmp_status = 'A' AND cmp_type = 'C' AND cp.textfield5 IN(
    select a.textfield5 from [483]..cicntp a  with (NOLOCK) group by a.textfield5 having count(*) &gt; 1)

UNION
SELECT '484' AS DB, cp.textfield5, cp.cnt_f_name, cp.cnt_l_name, cp.FullName,
c.cmp_code, c.cmp_name, c.cmp_status, c.cmp_type
FROM [484]..cicntp cp with (NOLOCK) 
INNER JOIN [484]..cicmpy c with (NOLOCK) on c.cmp_wwn = cp.cmp_wwn
WHERE cmp_status = 'A' AND cmp_type = 'C' AND cp.textfield5 IN(
    select a.textfield5 from [484]..cicntp a  with (NOLOCK) group by a.textfield5 having count(*) &gt; 1)

ORDER BY DB, c.cmp_code, cp.textfield5</datasource><contentbody>&lt;!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN"&gt;
